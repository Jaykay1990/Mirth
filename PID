//For each MRN in PID 3 map as needed  
for each (ptld in msg['PID']['PID.3']) 

{

	var idType = ptId['PID. 3.5'].toString(); 

	var id = ptId ['PID.3.1'].toString();
 
	
	if (idType == "iPM_ID")
	{
		var matchid = id;
	}

	if (idType == "MRN")
	{
		var hno = id;
	}
}
msg['PID']['PID.2']['PID2.1'] = matchid;

msg['PID']['PID.3'] = <PID.3/>;	
msg['PID']['PID.3']['PID3.1'] = hno;
